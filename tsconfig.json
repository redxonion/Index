{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
        "paths": {
        "@/layouts/*": ["app/layouts/*"],
       "@/storeMdx/*": ["src/context/*"],
      "@/renderMdxconfig/*": ["src/buckets/config/*"],
      "@/renderMdxprops/*": ["src/buckets/props/*"],
      "@/renderMdxutils/*": ["src/buckets/utils/*"],
            "styles/*": [ "styles/*" ]
        }
  },
  "exclude": ["node_modules", ".next", "out"],
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "**/*.js","**/*.jsx"]
}